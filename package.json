{
  "name": "kicker_replay",
  "version": "0.0.1",
  "main": "dist/app.js",
  "license": "MIT",
  "author": "Pom4H",
  "private": true,
  "scripts": {
    "start": "bin/environment node dist/app.js | pino-pretty -c -t",
    "start-dev": "KICKER_REPLAY_ENV=dev node_modules/.bin/nodemon dist/app.js | pino-pretty -c -t",
    "compile": "rm -rf dist && ./node_modules/.bin/tsc",
    "compile-dev": "rm -rf dist && ./node_modules/.bin/tsc --watch",
    "tslint": "./node_modules/.bin/tslint 'src/**/*.ts'",
    "migration:run": "bin/environment ./node_modules/.bin/typeorm migration:run",
    "migration:undo": "bin/environment ./node_modules/.bin/typeorm migration:revert",
    "migration:create": "bin/environment ./node_modules/.bin/typeorm migration:create -n",
    "orm-entity:create": "bin/environment ./node_modules/.bin/typeorm entity:create -n",
    "test": "mocha --timeout 10000 dist/test/unit/index.js"
  },
  "dependencies": {
    "@types/chai": "4.1.7",
    "@types/express": "4.16.1",
    "@types/express-session": "1.15.12",
    "@types/lodash": "4.14.123",
    "@types/minimist": "1.2.0",
    "@types/mocha": "5.2.6",
    "@types/node": "10.12.2",
    "@types/pino": "5.8.6",
    "@types/randomstring": "1.1.6",
    "@types/rimraf": "^2.0.2",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "chai": "4.2.0",
    "class-transformer": "0.2.0",
    "diskusage": "^1.0.0",
    "express": "4.16.4",
    "express-pino-logger": "4.0.0",
    "inversify": "5.0.1",
    "inversify-inject-decorators": "3.1.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "mocha": "6.1.2",
    "pg": "7.9.0",
    "pino": "5.12.2",
    "randomstring": "1.1.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "^2.6.3",
    "routing-controllers": "0.7.7",
    "source-map-support": "0.5.12",
    "tsconfig-paths": "3.8.0",
    "typeorm": "0.2.16",
    "typescript": "3.4.3"
  },
  "devDependencies": {
    "nodemon": "1.18.11",
    "pino-pretty": "2.6.0",
    "tslint": "5.15.0",
    "tslint-config-airbnb": "5.11.0"
  }
}
